        "gulp-git": "^2.10.1",
        "gulp-if": "3.x.x",
        "gulp-imagemin": "7.x.x",
        "gulp-json-transform": "^0.4.7",
        "gulp-less": "4.x.x",
        "gulp-livereload": "4.x.x",
